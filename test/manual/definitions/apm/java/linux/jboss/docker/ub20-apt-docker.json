    {
      "id": "jboss",
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/jboss/docker/debian/noApp/roles",
      "port": 9090,
      "destinations": ["dockerJboss"]
