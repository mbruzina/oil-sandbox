{
  "global_tags": {
    "owning_team": "OpenSource",
    "Environment": "development",
    "Department": "Product",
    "Product": "Virtuoso"
  },
  "resources": [{
    "id": "host",
    "display_name": "AwsLinux2InfraJavaInstallHost",
    "provider": "aws",
    "type": "ec2",
    "size": "t2.micro",
    "ami_name": "amazonlinux-2-base*",
    "user_name": "ec2-user"
  }],
  "services": [
    {
      "id": "docker",
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/docker/install/roles",
      "port": 9999,
      "destinations": [
        "host"
      ]
    },
    {
      "id": "jetty",
      "local_source_path": "/mnt/deployer/open-install-library",
      "deploy_script_path": "test/deploy/linux/jetty/docker/install/roles",
      "port": 9090,
      "destinations": [
        "host"
      ]
    }
  ]
}
